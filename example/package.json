{
    "name": "epinova-webpack-example",
    "version": "1.4.3-rc1",
    "description": "",
    "main": "test.js",
    "scripts": {
        "build": "webpack --mode production --config webpack.config.js",
        "build:watch": "webpack --mode development --watch",
        "build:develop": "webpack --mode development",
        "build:server-react": "webpack --mode production --config webpack.config.react.server.js",
        "build:server-vue": "webpack --mode production --config webpack.config.vue.server.js",
        "develop": "concurrently -n client,react,vue \"npm run develop:client\" \"npm run develop:server-react\" \"npm run develop:server-vue\"",
        "develop:client": "webpack serve --mode development --config webpack.config.client.js",
        "develop:server-react": "webpack watch --mode development --config webpack.config.react.server.js",
        "develop:server-vue": "webpack watch --mode development --config webpack.config.vue.server.js"
    },
    "engines": {
        "node": ">=16"
    },
    "engineStrict": true,
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "normalize-scss": "^7.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "vue": "^2.6.14",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@epinova/webpack": "file:../",
        "@types/react": "^17.0.44",
        "@types/react-dom": "^17.0.15",
        "concurrently": "^7.6.0",
        "copy-webpack-plugin": "^6.4.1",
        "css-loader": "^5.2.7",
        "svg-spritemap-webpack-plugin": "^3.9.1",
        "vue-loader": "^15.9.8",
        "vue-server-renderer": "^2.6.14",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "webpack-dev-server": "^4.11.1"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "cover 95% in NO"
    ]
}
